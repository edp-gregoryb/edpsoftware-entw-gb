{
  "name": "edpSoftware",
  "version": "10.0.3",
  "license": "EDP",
  "angular-cli": {},
  "engines": {
    "node": "12.x"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build-locale": "ng build --prod && npm run versioning",
    "lint": "tslint \"src/**/*.ts\"",
    "test": "ng test",
    "versioning": "cp CHANGELOG.md dist/assets",
    "pree2e": "webdriver-manager update",
    "e2e": "protractor",
    "i18n": "ng-xi18n -p src/tsconfig.json",
    "extract-i18n": "ng xi18n --output-path locale --i18n-locale de && xliffmerge --profile xliffmerge.json fr it",
    "build-locale:de": "ng build --prod --i18n-locale de --i18n-format xlf --i18n-file src/locale/messages.xlf --output-path=dist/de --baseHref /de/",
    "build-locale:fr": "ng build --prod --i18n-locale fr --i18n-format xlf --i18n-file src/locale/messages.fr.xlf --output-path=dist/fr --baseHref /fr/",
    "build-locale:it": "ng build --prod --i18n-locale it --i18n-format xlf --i18n-file src/locale/messages.it.xlf --output-path=dist/it --baseHref /it/",
    "build": "npm run build-locale:fr && npm run build-locale:it && npm run build-locale:de",
    "doversion": "npx angular-release  && npm run versioning",
    "now-build": "ng build --prod"
  },
  "private": true,
  "dependencies": {
    "@agm/core": "^1.0.0-beta.3",
    "@angular-devkit/build-angular": "^0.803.25",
